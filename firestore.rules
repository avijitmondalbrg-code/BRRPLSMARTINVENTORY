
rules_version = '2';

// CAUTION: These rules allow anyone with your project ID to read and write to your database.
// This is intended for initial setup and development.
// Before moving to production, please implement proper authentication-based rules.

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
